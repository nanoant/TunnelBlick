/* Window text */
" Do you wish to replace\n    %@\n    in %@\nwith %@%@?\n\n%@" = " Přejete si přepsat\n    %1$@\n    v %2$@\n obsahem %3$@%4$@?\n\n%5$@";

/* Window text */
" Tunnelblick cannot be used from this location. It must be installed on a local hard drive.\n\n" = " Tunnelblick nelze používat z této složky. Musí být nainstalován na lokálním disku.\n\n";

